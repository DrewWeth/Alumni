<%= link_to "Claim Org", :controller => :orgs, :action => :claim %>

<% if user_logged_in? %>
<table>
  <thead>
    <tr>
      <th>Members</th>
      
    </tr>
  </thead>

  <tbody>
    <% @members.each do |mem| %>
      <tr>
        <td><%= mem.f_name %></td>
        </tr>
    <% end %>
  </tbody>
</table>
<% end>

<%= link_to 'Edit', edit_org_path(@org) %> |
